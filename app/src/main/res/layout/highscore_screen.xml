<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/screensaver_solarwinds"
    android:id="@+id/hsscreen_layout"
    tools:context=".HighScore_Screen" >

    <ImageView
        android:id="@+id/hScores"
        android:contentDescription="HS Logo"
        android:src="@drawable/chighscores"
        android:layout_width="400dp"
        android:layout_height="120dp"
        android:layout_gravity="center"
        />

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="500dp"
        android:layout_margin="5dp"
        android:padding="5dp"
        android:gravity="top"
        android:layout_weight="1"
        android:stretchColumns="*"
        android:id="@+id/highscoren_layout" >







    </TableLayout>

    <Button
        android:id="@+id/e_button4"
        android:contentDescription="Back To Main Menu"
        android:layout_width="200dp"
        android:layout_height="50dp"
        android:layout_gravity="center"
        android:text="Back To Main Menu" />


</LinearLayout>
